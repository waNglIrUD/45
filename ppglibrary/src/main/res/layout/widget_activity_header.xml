<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/top_navigate"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroud_theme"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/NavigateBackLinearLayout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center_vertical"
        android:gravity="center"
        android:clickable="true"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_1"
        android:paddingRight="@dimen/padding_1"
        android:visibility="visible">

    <!--<LinearLayout-->
        <!--android:id="@+id/NavigateBackLinearLayout"-->
        <!--android:layout_width="@dimen/icon_size_nav_home_w"-->
        <!--android:layout_height="match_parent"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_centerVertical="true"-->
        <!--android:layout_gravity="center_vertical"-->
        <!--android:gravity="center"-->
        <!--android:clickable="true"-->
        <!--android:orientation="horizontal"-->
        <!--android:paddingLeft="-15dp"-->
        <!--android:paddingRight="@dimen/padding_1"-->
        <!--android:background="@drawable/top_bar_button"-->
        <!--android:visibility="visible">-->

        <!--<ImageView-->
            <!--android:id="@+id/NavigateOperation_L1"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:adjustViewBounds="true"-->
            <!--android:clickable="true"-->
            <!--android:gravity="center"-->
            <!--android:scaleType="center"-->
            <!--android:src="@drawable/left_arrow"-->
            <!--android:layout_marginLeft="5dp"-->
            <!--android:textColor="@color/white0"-->
            <!--android:visibility="visible" />-->

        <ImageView
            android:id="@+id/NavigateOperation_L1"
            android:layout_width="@dimen/icon_size_nav_back_w"
            android:layout_height="match_parent"
            android:adjustViewBounds="true"
            android:background="@color/transparent"
            android:gravity="center"
            android:clickable="false"
            android:scaleType="centerInside"
            android:src="@drawable/left_arrow"
            android:textColor="@color/white0"
            android:visibility="visible" />

        <TextView
            android:id="@+id/NavigateOperation_L2"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:adjustViewBounds="true"
            android:background="@color/transparent"
            android:gravity="center"
            android:clickable="false"
            android:scaleType="center"
            android:textColor="@color/white0"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/NavigateBackLinearLayout_joint"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center_vertical"
        android:background="@drawable/top_bar_button"
        android:clickable="true"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="6dp"
        android:paddingRight="8dp"
        android:visibility="gone">

        <ImageView
            android:id="@+id/NavigateOperation_L1_joint"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:adjustViewBounds="true"
            android:background="#00000000"
            android:clickable="false"
            android:scaleType="center"
            android:src="@drawable/top_bar_icon_back"
            android:textColor="@color/white0" />

        <TextView
            android:id="@+id/NavigateOperation_L2_joint"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="2dp"
            android:adjustViewBounds="true"
            android:clickable="false"
            android:gravity="center|left"
            android:scaleType="center"
            android:text="返回"
            android:textColor="@color/white0" />
    </LinearLayout>

    <TextView
        android:id="@+id/NavigateTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="75dp"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_head_title" />

    <!-- 选择模块 -->

    <LinearLayout
        android:id="@+id/select_frame"
        android:layout_width="115dp"
        android:layout_height="33dp"
        android:layout_centerInParent="true"
        android:layout_centerVertical="true"
        android:background="@drawable/button_stroke_1"
        android:orientation="horizontal"
        android:visibility="gone">

        <FrameLayout
            android:id="@+id/select_frame_1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:id="@+id/select_frame_1_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textColor="@color/white"
                android:textSize="17sp" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/select_frame_2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:id="@+id/select_frame_2_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textColor="@color/white"
                android:textSize="17sp" />
        </FrameLayout>
    </LinearLayout>

    <EditText
        android:id="@+id/search_detail_edittext"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="55dp"
        android:layout_marginRight="50dp"
        android:layout_marginTop="5dp"
        android:hint="输入关键字"
        android:singleLine="true"
        android:textColor="@color/white"
        android:textColorHint="@color/gray5"
        android:visibility="gone" />

    <LinearLayout
        android:id="@+id/NavigateHomeLinearLayout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center_vertical"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_small">

        <TextView
            android:id="@+id/NavigateOperation_R3"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:background="@drawable/top_bar_button"
            android:clickable="true"
            android:gravity="center"
            android:adjustViewBounds="true"
            android:scaleType="center"
            android:paddingLeft="5dp"
            android:layout_marginRight="-6dp"
            android:textColor="@color/white"
            android:visibility="gone" />

        <ImageButton
            android:id="@+id/NavigateOperation_R2"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/top_bar_chunk_space"
            android:adjustViewBounds="true"
            android:background="@drawable/top_bar_button"
            android:clickable="true"
            android:gravity="center"
            android:scaleType="center"
            android:textColor="@color/white0"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/NavigateOperation_R6"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginRight="0dp"
            android:adjustViewBounds="true"
            android:background="@drawable/top_bar_button"
            android:clickable="true"
            android:gravity="center"
            android:paddingLeft="@dimen/padding_small"
            android:visibility="gone">

            <TextView
                android:layout_width="78dp"
                android:layout_height="25dp"
                android:layout_marginRight="@dimen/top_bar_chunk_space"
                android:background="@drawable/rounded_notice"
                android:gravity="center"
                android:textSize="12sp"
                android:text=" + 货源通知"
                android:textColor="@color/yellow2"
                />

        </LinearLayout>




        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/NavigateOperation_R5"
            android:background="@drawable/top_bar_button"
            android:paddingRight="10dp"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/NavigateOperation_R5_Iv"
                android:layout_width="22dp"
                android:layout_height="match_parent"
                android:layout_marginRight="-2dp"
                android:adjustViewBounds="true"
                android:gravity="center"
                android:scaleType="center"
                android:clickable="false"
                android:background="#00000000"
                android:textColor="@color/white0"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/NavigateOperation_R5_Tv"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:clickable="false"
                android:paddingLeft="0dp"
                android:textSize="18sp"
                android:background="#00000000"
                android:textColor="@color/yellow2"
                android:visibility="gone" />

        </LinearLayout>

        <ImageButton
            android:id="@+id/NavigateOperation_R1"
            android:layout_width="@dimen/icon_size_nav_home_w"
            android:layout_height="match_parent"
            android:layout_marginRight="0dp"
            android:adjustViewBounds="true"
            android:background="@drawable/top_bar_button"
            android:clickable="true"
            android:gravity="center"
            android:scaleType="center"
            android:textColor="@color/white0"
            android:visibility="gone" />

    </LinearLayout>

    <Button
        android:id="@+id/NavigateOperation_R4"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="2dp"
        android:adjustViewBounds="true"
        android:background="@drawable/shape_oval"
        android:clickable="true"
        android:scaleType="center"
        android:text="111"


        android:visibility="gone" />
</RelativeLayout>